'''
选择排序
'''
def selectSort(li):
    for i in range(len(li)):
        minnum = i
        for j in range(i+1,len(li)):
            if li[minnum] > li[j]:
                minnum = j
        li[minnum],li[i] = li[i],li[minnum]
    return li
if __name__ =='__main__':
    a = [14,12,15,13,19,16,20]
    print(a)
    selectSort(a)
    print(a)
