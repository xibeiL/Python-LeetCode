# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution:
    def isPalindrome(self, head: ListNode) -> bool:
        if head is None:
            return True
        li = []
        i = head
        while i :
            li.append(i.val)
            i = i.next
        return li == li[::-1]
