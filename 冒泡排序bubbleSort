def bubbleSort(li):
    j = 0
    while j < len(li) - 1:
        count = 0
        for i in range(len(li)-1-j):
            if li[i] > li[i+1]:
                li[i],li[i+1] = li[i+1],li[i]
                count = 1
        if not count:
            return li
        j += 1
    return li
if __name__ =='__main__':
    a = [14,12,15,13,19,16,20]
    print(a)
    bubbleSort(a)
    print(a)
